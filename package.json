{
  "name": "vim",
  "displayName": "Vim",
  "description": "Vim emulation for Visual Studio Code",
  "icon": "images/icon.png",
  "version": "0.0.16",
  "publisher": "vscodevim",
  "galleryBanner": {
    "color": "#a5c9a2",
    "theme": "light"
  },
  "license": "MIT",
  "keywords": [
    "vim",
    "vi",
    "vscodevim",
    "vsc-vim"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/VSCodeVim/Vim.git"
  },
  "homepage": "https://github.com/VSCodeVim/Vim",
  "bugs": {
    "url": "https://github.com/VSCodeVim/Vim/issues"
  },
  "engines": {
    "vscode": "^0.10.12"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
        { "command": "extension.showCmdLine", "title": "Vim: Show Command Line" }
    ],
    "keybindings": [
        { "key": "Escape", "command": "extension.vim_esc", "when": "editorTextFocus" }
    ],
    "configuration": {
      "title": "Vim Configuration",
      "type": "object",
      "properties": {
        "vim.keyboardLayout": {
          "default": "en-US (QWERTY)",
          "type": "string",
          "description": "Keyboard layout to use to translated key presses."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "test": "node ./node_modules/vscode/bin/test",
    "postinstall": "node ./node_modules/vscode/bin/install && gulp init"
  },
  "dependencies": {
    "lodash": "^4.5.1",
    "copy-paste": "^1.1.4"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-bump": "^2.0.1",
    "gulp-filter": "^4.0.0",
    "gulp-git": "^1.7.0",
    "gulp-mocha": "^2.2.0",
    "gulp-shell": "^0.5.2",
    "gulp-soften": "^0.0.1",
    "gulp-tag-version": "^1.3.0",
    "gulp-trimlines": "^1.0.0",
    "gulp-tslint": "^4.3.2",
    "gulp-typescript": "^2.12.0",
    "gulp-typings": "^1.1.0",
    "tslint": "^3.5.0",
    "typescript": "^1.8.2",
    "typings": "^0.6.8",
    "vscode": "^0.11.1"
  }
}
